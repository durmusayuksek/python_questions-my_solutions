    "\n",
    "res = []\n",
    "dictt = {}\n",
    "n = 1000000\n",
    "zar = [1,2,3,4,5,6]\n",
    "\n",
    "for i in range(n):\n",
    "    a = random.choice(zar)\n",
    "    res.append(a)\n",
    "    \n",
    "for i in res:\n",
    "    dictt[i] = dictt.get(i, 0) + 1\n",
    "\n",
    "for k,v in dictt.items():\n",
    "    print(f'{k}: % {v/n*100}')"
