   ],
   "source": [
    "liste = [\n",
    "  [1, 0, 2],\n",
    "  [5, 5, 7],\n",
    "  [9, 4, 3]\n",
    "]\n",
    "result = 0\n",
    "for i in liste:\n",
    "    for j in i:\n",
    "        if not j % 2:\n",
    "            result += j\n",
    "print(result)"
